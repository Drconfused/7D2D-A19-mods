	<!-- Ammo Piles -->
	<append xpath="/blocks">
	<!-- 	Ammo Pile Small cntAmmoPileSmall -->
		<block name="drsAmmoPileSmall">
			<property name="CustomIcon" value="cntAmmoPileSmall"/>
			<property name="Material" value="Mtrash"/>
			<property name="Shape" value="ModelEntity"/>
			<property name="Model" value="#Entities/LootContainers?AmmoPiles_SmallPrefab.prefab"/>
			<property name="Collide" value="melee,bullet,arrow,rocket"/>
			<property name="OnlySimpleRotations" value="true"/>
			<property name="ImposterExclude" value="true"/>
			<property name="IsTerrainDecoration" value="true"/>
			<property name="CanPickup" value="true" param1="lootBoxAmmoC"/>
			<drop event="Destroy" name="lootBoxAmmoC" count="1"/>
			<property name="ShowModelOnFall" value="false"/>
			<property name="FilterTags" value="floot,ftrash"/>
		</block>	
	<!-- 	Ammo Pile Medium cntAmmoPileMedium -->
		<block name="drsAmmoPileMedium">
			<property name="Extends" value="drsAmmoPileSmall"/>
			<property name="CustomIcon" value="cntAmmoPileMedium"/>
			<property name="Model" value="#Entities/LootContainers?AmmoPiles_MediumPrefab.prefab"/>
			<property name="CanPickup" value="true" param1="lootBoxAmmoB"/>
			<drop event="Destroy" name="lootBoxAmmoB" count="1"/>
		</block>				
	<!-- 	Ammo Pile Large cntAmmoPileLarge -->
		<block name="drsAmmoPileLarge">
			<property name="Extends" value="drsAmmoPileSmall"/>
			<property name="CustomIcon" value="cntAmmoPileLarge"/>
			<property name="Model" value="#Entities/LootContainers?AmmoPiles_LargePrefab.prefab"/>
			<property name="MultiBlockDim" value="2,1,1"/><property name="DisplayType" value="blockMulti"/>
			<property name="ModelOffset" value=".5,.5,0"/>
			<property name="CanPickup" value="true" param1="lootBoxAmmoA"/>
			<drop event="Destroy" name="lootBoxAmmoA" count="1"/>
		</block>		
	</append>

